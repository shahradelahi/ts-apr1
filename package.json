{
  "name": "@se-oss/apr1",
  "version": "0.0.0",
  "description": "Apache's APR1 MD5 password hashing for Node.js and browsers.",
  "keywords": [
    "apr1",
    "md5",
    "hash",
    "htpasswd",
    "isomorphic"
  ],
  "homepage": "https://github.com/shahradelahi/ts-apr1",
  "repository": "github:shahradelahi/ts-apr1",
  "license": "MIT",
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/browser.d.ts",
      "default": {
        "import": "./dist/browser.js",
        "require": "./dist/browser.cjs"
      }
    }
  },
  "main": "dist/browser.cjs",
  "module": "dist/browser.js",
  "types": "dist/browser.d.ts",
  "files": [
    "dist/**",
    "!**/*.d.cts"
  ],
  "scripts": {
    "bench": "vitest bench --run",
    "build": "pnpm typecheck && tsup",
    "clean": "git clean -dfx **/node_modules/** dist .tsbuildinfo",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "pnpm typecheck && eslint .",
    "lint:fix": "eslint --fix .",
    "prepublishOnly": "pnpm build && pnpm lint && pnpm format:check && pnpm test",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@shahrad/prettier-config",
  "dependencies": {
    "@se-oss/md5": "^1.1.1",
    "@se-oss/timing-safe-compare": "^1.0.0"
  },
  "devDependencies": {
    "@shahrad/eslint-config": "^1.0.1",
    "@shahrad/prettier-config": "^1.2.2",
    "@shahrad/tsconfig": "^1.2.0",
    "@types/node": "^25.2.2",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.29.2+sha512.bef43fa759d91fd2da4b319a5a0d13ef7a45bb985a3d7342058470f9d2051a3ba8674e629672654686ef9443ad13a82da2beb9eeb3e0221c87b8154fff9d74b8"
}
